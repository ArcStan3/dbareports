  <DataSets>
    <DataSet Name="DatabaseFileSizes">
      <Query>
        <DataSourceName>SQLInfo</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ServerName">
            <Value>=Parameters!ServerName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@History">
            <Value>=Parameters!History.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DatabaseFileSizes.ServerName
  ,DatabaseFileSizes.DatabaseName
  ,DatabaseFileSizes.LogicalFileName
  ,DatabaseFileSizes.CheckDate
  ,DatabaseFileSizes.SizeInKB
  ,DatabaseFileSizes.DBFileName
FROM
  DatabaseFileSizes
WHERE
  DatabaseFileSizes.DBFileName LIKE N'%.ldf'
AND
ServerName = @ServerName 
AND
CheckDate &gt; (DATEADD(DAY,@History,CONVERT(DATE,GETDATE())))</CommandText>
        <rd:DesignerState><QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational"><SelectedColumns><ColumnExpression ColumnOwner="DatabaseFileSizes" ColumnName="ServerName" /><ColumnExpression ColumnOwner="DatabaseFileSizes" ColumnName="DatabaseName" /><ColumnExpression ColumnOwner="DatabaseFileSizes" ColumnName="LogicalFileName" /><ColumnExpression ColumnOwner="DatabaseFileSizes" ColumnName="CheckDate" /><ColumnExpression ColumnOwner="DatabaseFileSizes" ColumnName="SizeInKB" /><ColumnExpression ColumnOwner="DatabaseFileSizes" ColumnName="DBFileName" /></SelectedColumns><Filters><Filter Operator="LIKE"><ColumnExpression ColumnOwner="DatabaseFileSizes" ColumnName="DBFileName" /><FilterValue>*.ldf</FilterValue></Filter></Filters></QueryDefinition></rd:DesignerState>
