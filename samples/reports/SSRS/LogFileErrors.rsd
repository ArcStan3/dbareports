<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition">
  <DataSet Name="">
    <Query>
      <DataSourceReference>DBA Database</DataSourceReference>
      <CommandText>SELECT 
[Date]
[FileName]
      ,[ErrorMsg]
      ,[Line]
	  ,[Matches]
  FROM [Info].[LogFileErrorMessages]
ORDER BY Date Desc, FileName</CommandText>
    </Query>
  </DataSet>
</SharedDataSet>